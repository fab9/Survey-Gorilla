<h3>Create New Survey</h3>

<form method="post" action="/survey/new" entype="multipart/form-data">
  <label>Survey Name:
    <input type="text" name="survey_name">
  </label>
  <br>
  <label>Image:
    <input type="file" name="image_url" accept="image/*">
  </label>
  <br>
  <%= erb :"partials/_question" %>

  <a href="#" id="add_question">Add Question</a>

  <br><br><input type='submit' value='submit'>

</form>

